@model UAndes.ICC5103._202301.Models.EnajenacionViewModel


@{
    ViewBag.Title = "Index";

}



<h2>INFORMACIÓN GENERAL</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Enajenacion.CNE)
        </th>
            
        <th>
            @Html.DisplayNameFor(model => model.Enajenacion.Comuna)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.Enajenacion.Manzana)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.Enajenacion.Predio)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.Enajenacion.Fojas)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.Enajenacion.FechaInscripcion)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.Enajenacion.IdInscripcion)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model.Enajenacions)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => Model.Descripcion[item.CNE])
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.Comuna)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.Manzana)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.Predio)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.Fojas)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.FechaInscripcion)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.IdInscripcion)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id, option = 'a', insideId = 0 })
            </td>
        </tr>
    }

</table>
